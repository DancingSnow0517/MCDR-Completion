{
  "schemaVersion": 1,
  "id": "${id}",
  "version": "${version}",

  "name": "${name}",
  "description": "Template for my fabric mods",
  "authors": [
    "DancingSnow"
  ],
  "contact": {
    "homepage": "https://modrinth.com",
    "sources": "https://github.com/DancingSnow0517/MCDR-Completion"
  },

  "icon": "assets/mcdrc/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "cn.dancingsnow.mcdrc.MCDRCompletion"
    ],
    "client": [
      "cn.dancingsnow.mcdrc.client.MCDRCompletionClient"
    ],
    "server": [
      "cn.dancingsnow.mcdrc.server.MCDRCompletionServer"
    ]
  },
  "mixins": [
    "mcdrc.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.11.4",
    "fabric": "*",
    "minecraft": "${minecraft_dependency}"
  }
}
